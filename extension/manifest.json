{
  "manifest_version": 3,
  "name": "GMAT Study Tracker",
  "version": "1.0.0",
  "description": "Quick study session logging with AI insights",
  "permissions": ["storage", "notifications"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  }
}
